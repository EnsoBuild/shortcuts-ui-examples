{
  "name": "@ensofinance/sdk",
  "version": "1.0.7",
  "description": "SDK for interacting with the Enso API",
  "author": "Enso Finance",
  "homepage": "https://www.enso.finance/",
  "main": "dist/index.js",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
